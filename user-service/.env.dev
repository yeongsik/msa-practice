# Development Environment Variables
# Database
DB_HOST=dev-postgres.internal
DB_PORT=5432
DB_NAME=twitterdb_dev
DB_USERNAME=twitter_dev
DB_PASSWORD=dev_secure_password

# Redis
REDIS_HOST=dev-redis.internal
REDIS_PORT=6379
REDIS_PASSWORD=dev_redis_secure_password

# Kafka
KAFKA_BROKERS=dev-kafka-1.internal:9092,dev-kafka-2.internal:9092

# Security
JWT_SECRET=dev-jwt-secret-key-should-be-secure

# File Storage
S3_ENDPOINT=http://dev-minio.internal:9000
S3_ACCESS_KEY=dev_access_key
S3_SECRET_KEY=dev_secret_key
S3_BUCKET=twitter-dev

# Monitoring
PROMETHEUS_ENABLED=true